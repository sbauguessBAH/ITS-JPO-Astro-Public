---
import banner from "@/src/assets/images/banners/AI_Banner.png";
import PageLayout from "@/src/layouts/PageLayout.astro";
import SidebarLayout from "@/src/layouts/SidebarLayout.astro";
import logo from "@/src/assets/images/AI/AI-TPD-Logo.png";

const keywords = ["Complete Streets Artificial Intelligence", "CSAI", "Transportation Research"];
---

<style>
  .CardTitle {
    font-weight: 900;
    margin-top: 20px;
    line-height: 28px;
    font-weight: bolder;
    font-size: 20px;
    color: #005195;
    min-height: 40px;
    display: flex;
    justify-content: center;
    align-items: center;
  }
  .CardTitle.White {
    color: white;
  }

  .table td {
    padding: 0.75rem;
    vertical-align: top;
    border-top: 5px solid #006d9b;
    border-bottom-color: white;
  }

  .tr-Color {
    border-top: 5px solid #006d9b;
    border-bottom-color: white;
  }

  /* Topbar */

  .Topbar {
    background-color: #1d3957;
    height: 50px;
  }

  .TopbarLogoTransportation {
    height: 25px;
    float: left;
    padding-right: 10px;
    margin-top: 14px;
  }

  .TopbarLogoTwitter {
    height: 15px;
    float: right;
    margin-right: 10px;
    margin-top: 17px;
  }

  .TopbarLogoITSJPO {
    height: 15px;
    float: right;
    margin-right: 10px;
    margin-top: 17px;
  }

  .TopbarLogoUSDOT {
    height: 15px;
    float: right;
    margin-right: 10px;
    margin-top: 17px;
  }

  /* Header */

  .HeaderTitle {
    font-weight: 800;
    color: #ffffff;
    margin-bottom: 0px;
  }

  .HeaderSubtitle {
    font-size: 22px;
    color: #ffffff;
    font-weight: 400;
  }

  .BannerFadedSummaryText {
    margin-top: 70px;
  }

  .RegisterNowButtonDiv {
    display: flex;
    justify-content: center;
    align-items: center;
  }
  .Homebutton {
    background-color: #1e3a5867;
    border-radius: 20px;
    border: none;
    color: white;
    padding: 10px 20px 10px 20px;
  }
  .Homebutton:hover {
    background-color: #1e3a58;
    border-radius: 20px;
    border: none;
    color: white;
    padding: 10px 20px 10px 20px;
  }

  .Registerbutton {
    background-image: linear-gradient(#01a0e9, #008bf5);
    border: none;
    font-weight: 900;
    color: rgb(255, 255, 255);
    padding: 10px 20px;
    padding-top: 10px;
    padding-right: 60px;
    padding-bottom: 10px;
    padding-left: 60px;
    text-align: center;
    text-decoration: none;
    display: inline-block;
    margin: 4px 2px;
    cursor: pointer;
    border-radius: 16px;
  }
  .RegisterbuttonWhite {
    background-color: white;
    border: none;
    font-weight: 900;
    color: #005195;
    padding: 10px 20px;
    padding-top: 10px;
    padding-right: 60px;
    padding-bottom: 10px;
    padding-left: 60px;
    text-align: center;
    text-decoration: none;
    display: inline-block;
    margin: 4px 2px;
    cursor: pointer;
    border-radius: 16px;
  }

  /* Table */

  .TableDate {
    font-size: 18px;
    font-weight: bolder;
    color: #024b80;
  }

  .TableTime {
    font-size: 16px;
    color: #024b80;
  }

  .TableContentTitle {
    font-size: 18px;
    font-weight: bolder;
    color: #024b80;
  }

  .TableSpeakerTitle {
    font-size: 16px;
    font-weight: bolder;
    color: #024b80;
  }

  .TableSpeakerDescription {
    color: #024b80;
    font-size: 16px;
  }

  .ConnectIcons {
    width: 90px;
    padding: 20px;
    align-content: center;
    text-align: center;
  }

  .vl {
    border-left: 2px solid rgb(184, 184, 184);
    height: 50px;
    float: left;
    margin-left: 10px;
    padding-right: 10px;
  }

  .vlTall {
    border-left: 2px solid rgb(184, 184, 184);
    height: 100%;
    float: left;
    margin-left: 10px;
    padding-right: 20px;
  }

  /* Purple Bar */

  .PurpleBar {
    height: 36px;
    width: 100%;
    background-image: linear-gradient(#01a0e9, #008bf5);
    float: left;
    border-radius: 25px;
    margin-top: 15px;
    font-size: large;
    color: white;
    padding-bottom: 10px;
    padding: 6px;
    font-weight: 600;
    position: absolute;
    left: 0;
    position: relative;
    margin-bottom: 10px;
  }

  .WhiteBar {
    height: 40px;
    width: 90%;
    background-color: #ffffff;
    border-radius: 25px;
    margin-top: 10px;
    font-size: large;
    color: white;
    padding-bottom: 10px;
    padding: 6px;
    font-weight: 600;
    left: 0;
    margin-bottom: 110px;
    margin-left: 10px;
  }

  .TitlePill {
    width: 100%;
    height: 40px;
    background-color: #ffffff;
    border: none;
    color: #005195;
    padding: 5px;
    font-weight: bolder;
    font-size: 20px;
    text-align: center;
    text-decoration: none;
    display: inline-block;
    margin: 50px 0px 25px 0px;
    margin-top: 20px;
    border-radius: 50px;
    text-align: center;
  }

  .InnerIcon-Left {
    width: 50px;
    position: absolute;
    left: 30px;
  }

  .InnerIcon-Right {
    width: 50px;
    position: absolute;
    z-index: 1;
    top: 50px;
    left: 50% + 250px;
  }

  .PurpleBarText {
    font-family: Arial, Helvetica, sans-serif;
    margin-left: 10px;
    margin-top: -4px;
    z-index: 100;
    position: absolute;
    font-size: 22px;
    color: white;
    font-weight: 900;
    padding-left: 6px;
  }

  .WhiteBarText {
    font-family: Arial, Helvetica, sans-serif;
    margin-left: 10px;
    margin-top: -3px;
    z-index: 100;
    font-size: 24px;
    color: #005195;
    font-weight: 900;
    padding-left: 6px;
  }

  .WhiteBarText {
    color: #005195;
  }

  .rightbar {
    position: absolute; /*or fixed*/
    right: 0px;
    height: 40px;
    background-color: #9c6298;
    float: left;
    width: 100%;
    width: auto;
  }
  .SocialMediaIcon {
    max-width: 50px;
  }

  .Imagecenter img {
    display: inline-block;
    margin: auto;
    border-radius: 18px, 0px, 0px, 18px;
  }
  .Popout {
    background-image: url("../Images/ITS_JPO_CompleteStreets_TileImages-06.png");
    background-position: center top;
  }

  .DisplayColumn {
    border-radius: 20px;
    border-color: #e4eef2;
    border-width: 2px;
    border-style: solid;
    background-color: #f5f6f7;
    padding: 0 0 20px 0;
    text-align: center;
    display: block;
    height: 100%;
  }
  .cardFlex {
    position: relative;
    height: 100%;
  }
  .subCardFlex {
    position: absolute;
    bottom: 0px;
  }

  .DisplayColumnline {
    border-width: 2px;
    background-color: rgb(206, 206, 206);
    height: 2px;
    opacity: 100%;
  }
  .Featured {
    border-color: #01a0e9;
    background-color: #167de3;
  }

  /* Engaement Pill */

  .EngangementPillRow {
    margin-bottom: 40px;
    padding-left: 20px;
    padding-right: 20px;
  }

  .EngangementTitle {
    margin-left: 10px;
    margin-top: 17px;
    z-index: 100;
    position: absolute;
    font-size: large;
    color: white;
    font-weight: 600;
  }

  .EngangementPillCol {
    padding: 15px;
  }

  .EngangementPill {
    width: 250px;
    height: 75px;
    background-color: rgb(255, 255, 255);
    border: none;
    color: black;
    padding: 10px 20px;
    padding-left: 10px;
    font-weight: 600;
    text-align: left;
    text-decoration: none;
    display: block;
    margin: auto;
    border-radius: 50px;
    box-shadow: 2px 2px 4px #88888872;
    border-style: solid;
    border-width: 3px;
    border-color: #88888872;
  }

  .EngangementPillIcon {
    width: 50px;
    height: 50px;
    border-radius: 25px;
    background-color: #12b8ff;
    float: left;
  }

  .EngangementPillText {
    margin-bottom: 20px;
    font-weight: bolder;
  }

  /* Footer */

  .footerdiv {
    margin-top: 20px;
    color: white;
    padding: 50px;
    background-color: rgb(19, 19, 19);
    text-align: center;
    font-size: 11px;
  }
  .footerdiv p {
    font-size: 11px;
  }

  .contactline {
    border-width: 2px;
    background-color: #005195;
    height: 3px;
    opacity: 100%;
    margin-top: 6px;
  }

  .contactlineFooter {
    border-width: 2px;
    background-color: white;
    height: 3px;
    opacity: 100%;
  }

  .InnerTitle {
    color: #005195;
    font-weight: bolder;
    font-size: 24px;
    text-align: left;
    margin-bottom: 6px;
  }
  .InnerSubtitle {
    font-family: Arial, Helvetica, sans-serif;
    color: #005195;
    font-weight: 600;
    font-size: 18px;
    text-align: left;
    margin-bottom: 0px;
  }

  /*Banner Faded */

  .BannerCol {
    margin-top: 20px;
  }

  .BannerFadedDiv {
    background-image: linear-gradient(#014384, #00376e);
    margin-top: 30px;
    margin-bottom: 10px;
    color: white;
    padding-bottom: 20px;
  }

  .BannerWhenWhyDiv {
    border-style: solid;

    border-color: white;
    border-radius: 20px;
    border-width: 3px;
    padding: 10px;
    background-color: #01a0e9;
  }

  .BannerWhenWhyDiv hr {
    height: 2px;
    border-width: 0;
    color: rgb(255, 255, 255);
    background-color: rgb(255, 255, 255);
    opacity: 1;
  }

  .BannerWhenWhyDiv p {
    font-size: 14px;
  }

  /* Banner */

  @media only screen and (max-width: 394px) {
    .banner {
      padding: 0px;
      background-image: url("../Images/Hero/CompleteStreets_HeroIMage_SX.png");
    }
  }

  @media only screen and (min-width: 394px) {
    .banner {
      padding: 0px;
      background-image: url("../Images/Hero/CompleteStreets_HeroIMage_SM.png");
    }
  }

  @media only screen and (min-width: 768px) {
    .banner {
      padding: 0px;
      background-image: url("../Images/Hero/CompleteStreets_HeroIMage_MD.png");
    }
  }

  @media only screen and (min-width: 960px) {
    .banner {
      padding: 0px;
      background-image: url("../Images/Hero/CompleteStreets_HeroIMage_MD.png");
    }
  }

  @media only screen and (min-width: 1440px) {
    .banner {
      padding: 0px;
      background-image: url("../Images/Hero/CompleteStreets_HeroIMage_LG.png");
    }
  }

  @media only screen and (min-width: 2000px) {
    .banner {
      padding: 0px;
      background-image: url("../Images/Hero/CompleteStreets_HeroIMage_LG.png");
    }
  }

  .InBetweenIcon {
    display: flex;
    justify-content: center;
    align-items: center;
    height: auto;
  }

  .ArrowIcon {
    width: 50px;
    height: 50px;
    content: url("../Images/ITS_IntersectionSafetyChallenge_Icons_Arrow-Down.png");
  }

  .CardInfoText_Solution {
  }
  .CardInfoText_Solution.White {
    color: white;
    padding-top: 10px;
  }

  @media only screen and (min-width: 990px) {
    .InBetweenIcon {
      min-height: 500px;
    }

    .ArrowIcon {
      content: url("../Images/ITS_IntersectionSafetyChallenge_Icons_Arrow-Right.png");
    }

    .CardInfoText {
      height: 177px;
    }

    .CardInfoText_Solution {
    }
  }
  @media only screen and (max-width: 500px) {
    .justify-content-start .col-lg-9 {
      display: none;
    }
    .justify-content-start .col-lg-3 {
      margin-top: 10px !important;
    }
    .justify-content-start .col-lg-3 button {
      width: 100%;
      margin-top: 0px;
    }
  }

  @media (min-width: 768px) {
    .seven-cols .col-lg-3 {
      width: 100%;
    }
  }

  @media (min-width: 992px) {
    .seven-cols .col-lg-3 {
      width: 22%;
      flex: none !important;
    }

    .seven-cols .col-lg-1 {
      flex: 0 0 8.333333%;
      max-width: 3.333333%;
    }
  }

  @media (min-width: 1200px) {
    .seven-cols .col-lg-3 {
      width: 22%;
      flex: none !important;
    }

    .seven-cols .col-lg-1 {
      flex: 0 0 8.333333%;
      max-width: 3.333333%;
    }
  }
  @media (maz-width: 1199px) {
  }
  .introBlue {
    padding: 10px 0px 0px 0px;
  }
  .BannerFadedDiv h1 {
    padding-left: 30px;
    font-family: Arial, Helvetica, sans-serif;
    font-weight: 600;
    font-size: 36px;
    margin: 0px !important;
    text-transform: none !important;
  }
  .bottomContainer {
    padding-left: 0px;
    padding-right: 0px;
    padding-bottom: 0px !important;
    background-image: linear-gradient(#00457d, rgb(7, 19, 38));
  }
  .popoutlink {
    font-size: 18px;
    font-family: sans-serif;
    font-weight: 900;
  }
  .popoutlink:hover {
    color: #63cafa;
  }
  .ContactUsTop {
    font-size: 16px !important;
    padding-left: 16px;
  }

  /* Accordion styling for the Datasets page */
  .Datasets_Resources a {
    color: #006d9b;
    font-weight: 600;
    text-decoration: underline;
  }

  .Datasets_Resources h2 {
    color: #014384;
    font-family: Arial, Helvetica, sans-serif;
    font-size: 16px;
    font-weight: 600;
    border-width: 2px;
  }
  .Datasets_Resources h3 {
    color: #014384;
    font-family: Arial, Helvetica, sans-serif;
    font-size: 16px;
    font-weight: 600;
  }
  .Datasets_Reources .container .row p {
    margin-bottom: 0px !important;
    padding-bottom: 0px !important;
  }
  .Datasets_Resources .accordion-header .accordion-button {
    color: #014384;
    font-family: Arial, Helvetica, sans-serif;
    font-size: 16px;
    font-weight: 300;
  }
  .Datasets_Resources .BannerFadedDiv {
    padding-bottom: 10px;
  }
  .Datasets_Resources p {
    margin-bottom: 0px !important;
  }

  .Datasets_Resources .accordion-body {
  }
  .Datasets_Resources .accordion-body p {
  }

  .Datasets_Resources .accordion-item li {
    color: #014384 !important;
  }

  .Datasets_Resources .accordion-button:not(.collapsed) {
    background-color: #0099d617;
    border-radius: 25px 25px 0px 0px;
    margin-top: 10px;
    font-weight: 600;
  }

  .Datasets_Resources .accordion-button.collapsed {
    margin-top: 10px;
    border-radius: 25px 25px 0px 0px;
  }
  .Datasets_Resources .accordion-button.collapsed:hover {
    background-color: #0099d617;
    border-radius: 25px 25px 0px 0px;
  }

  .accordionLevelOneBold {
    font-weight: 700 !important;
  }
  h2.accordionNoUnderline {
    border-bottom: 3px solid #00457d !important;
  }
  #accordionFlushExample {
    margin-bottom: 40px;
  }

  .externalLink:after {
    content: "\f08e";
    font-family: FontAwesome;
    font-style: normal;
    font-weight: normal;
    text-decoration: inherit;
    font-size: 12px;
    margin-left: 6px;
    color: #009ad6;
    -webkit-text-stroke: 0px #f8f8f8;
  }

  .externalLinkWhite:after {
    content: "\f08e";
    font-family: FontAwesome;
    font-style: normal;
    font-weight: normal;
    text-decoration: inherit;
    font-size: 12px;
    margin-left: 6px;
    color: #ffffff;
    -webkit-text-stroke: 0px #f8f8f8;
  }

  /* Rows so that the columns are balanced   */

  .row.supportCards {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
  }
  .row.supportCards > [class*="col-"] {
    display: flex;
    flex-direction: column;
  }
  .whiteBoldLink a {
    font-family: Arial, Helvetica, sans-serif;
    font-weight: 600;
    text-decoration: underline;
  }
  .whiteBoldLink a:hover {
    color: #5bb8ff;
  }

     .nlink {
      font-family: "Source Sans Pro", sans-serif;
      font-size: 17px;
      color: black;
    }
    .CardPadding {
      padding-top: 110px;
      padding-bottom: 0px !important;
    }
    .calloutImageHeader {
      width: 100%;
      border-radius: 18px 18px 0px 0px;
    }
    .list-group-item:first-child {
      border-top-left-radius: 25px;
      border-top-right-radius: 25px;
    }
    .list-group-item:last-child {
      border-bottom-left-radius: 25px;
      border-bottom-right-radius: 25px;
    }
    .fa-bullhorn {
      rotate: -20deg;
    }
    .fa-circle {
      color: #008bf5;
      filter: drop-shadow(0 0 .2rem gray);
    }
</style>

<PageLayout title="Complete Streets Artificial Intelligence" bannerImage={banner}>
  <Fragment slot="banner"> <img alt="ITS4US Logo" src={logo.src} style="object-fit: cover; height:100%; width:auto; max-height: 100px; clear: none" /></Fragment>
  <SidebarLayout path="/research-areas/">
    


    <div class="container">
          <div class="row">
            <div class="col-lg-12 BannerCol">
              <div class="WhiteBar">
                <p class="WhiteBarText">Vision</p>
              </div>

              <p class="BannerFadedSummaryText">
                The Complete Streets AI Initiative is a $15 million multi-phase effort funding small businesses to develop powerful new decision-support tool(s) for state, local and tribal
                transportation agencies that assists in the siting, design, and deployment of Complete Streets.
              </p>
            </div>

            
          </div>
        </div>
          <div class="container">
        

            <div class="row">
              <div class="col-lg-12">
                <div class="PurpleBar">
                  <p class="PurpleBarText">Announcements</p>
                </div>
              </div>
            </div>

            <div class="col-lg-12 mt-3">
              <div class="list-group">
                <a
                  href="https://www.its.dot.gov/communications/blogs/itsjpo_directors_blog_18.htm"
                  class="list-group-item list-group-item-action d-flex gap-3 py-3"
                  target="_blank"
                  rel="noopener noreferrer"
                >
                  <span class="fa-stack fa-lg">
                    <i class="fa-solid fa-circle fa-stack-2x" aria-hidden="true"></i>
                    <i class="fa-solid fa-bullhorn fa-stack-1x fa-inverse" aria-hidden="true"></i>
                  </span>
                  <div class="d-flex gap-2 w-100 justify-content-between">
                    <div>
                      <h6 class="mb-0">U.S. DOT Advances Complete Streets Through Artificial Intelligence: A Message from Brian Cronin</h6>
                      <p class="mb-0 opacity-75">ITS JPO Director's Blog</p>
                    </div>
                  </div>
                </a>
                <a
                  href="https://www.volpe.dot.gov/sites/volpe.dot.gov/files/2024-10/24.2%20SBIR%20Awardees_508.pdf"
                  class="list-group-item list-group-item-action d-flex gap-3 py-3"
                  target="_blank"
                  rel="noopener noreferrer"
                >
                  <span class="fa-stack fa-lg">
                    <i class="fa-solid fa-circle fa-stack-2x" aria-hidden="true"></i>
                    <i class="fa-solid fa-bullhorn fa-stack-1x fa-inverse" aria-hidden="true"></i>
                  </span>
                  <div class="d-flex gap-2 w-100 justify-content-between">
                    <div>
                      <h6 class="mb-0">Meet the 12 Phase I Awardees</h6>
                      <p class="mb-0 opacity-75">SBIR Fiscal Year 2024.2 Phase I Awards: Complete Streets Artificial Intelligence Initiative</p>
                    </div>
                    <small class="opacity-50 text-nowrap">PDF</small>
                  </div>
                </a>
                <a
                  href="https://www.transportation.gov/briefing-room/usdot-awards-2-million-12-small-businesses-csai-initiative"
                  class="list-group-item list-group-item-action d-flex gap-3 py-3"
                  target="_blank"
                  rel="noopener noreferrer"
                >
                  <span class="fa-stack fa-lg">
                    <i class="fa-solid fa-circle fa-stack-2x" aria-hidden="true"></i>
                    <i class="fa-solid fa-bullhorn fa-stack-1x fa-inverse" aria-hidden="true"></i>
                  </span>
                  <div class="d-flex gap-2 w-100 justify-content-between">
                    <div>
                      <h6 class="mb-0">U.S. DOT Awards $2.4 Million to 12 Small Businesses for the Complete Streets Artificial Intelligence Initiative</h6>
                      <p class="mb-0 opacity-75">U.S. Department of Transportation | Newsroom</p>
                    </div>
                    <small class="opacity-50 text-nowrap">External Link </small>
                  </div>
                </a>
                <a
                  href="https://www.volpe.dot.gov/work-us/small-business-innovation-research/us-dot-sbir-fiscal-year-20242-fy242-solicitation-now"
                  class="list-group-item list-group-item-action d-flex gap-3 py-3"
                  target="_blank"
                  rel="noopener noreferrer"
                >
                  <span class="fa-stack fa-lg">
                    <i class="fa-solid fa-circle fa-stack-2x" aria-hidden="true"></i>
                    <i class="fa-solid fa-bullhorn fa-stack-1x fa-inverse" aria-hidden="true"></i>
                  </span>
                  <div class="d-flex gap-2 w-100 justify-content-between">
                    <div>
                      <h6 class="mb-0">SBIR Solicitation for Complete Streets AI Initiative - Generating, Integrating, and Activating Data for Mobility</h6>
                      <p class="mb-0 opacity-75">Volpe Center | Small Business Innovation Research</p>
                    </div>
                    <small class="opacity-50 text-nowrap">External Link </small>
                  </div>
                </a>
              </div>
            </div>

            <div class="row">
              <div class="col-lg-12 mt-3">
                <div class="PurpleBar">
                  <p class="PurpleBarText">How Do We Get There?</p>
                </div>
              </div>
            </div>

            <div class="row EngangementPillRow">
              <div class="col-lg-7 mt-3">
                <p class="InnerTitle">Complete the Data Gap</p>
                <hr class="contactline" />
                <p style="margin-bottom: 0">
                  Complete Streets are easier to plan and build with more complete data. In some cases, basic roadway data such as traffic volume or the presence of sidewalks is unavailable or
                  incomplete. In other cases, data can be skewed demographically. To close these gaps, we encourage creative methods to develop national datasets using novel and scalable approaches to
                  data science such as utilizing big data, internet-of-things (IoT), artificial intelligence, computer vision and machine learning to a range of data sources and types that may include
                  satellite, aerial, or street-level video and still photography; satellite, aerial, or street-level lidar data; sensor-based and crowdsourced data; vehicle probe data and telemetry,
                  including shared micromobility and transit modes; multimodal volume/count data; and other sources and types.
                </p>
              </div>

