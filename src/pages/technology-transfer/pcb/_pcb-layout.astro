---
import PageLayout from "@/src/layouts/PageLayout.astro";

interface Props {
  title: string;
}

const { title } = Astro.props;
---
<style is:global>
  section {
    padding: 0;
    margin: 0;
  }
</style>
<PageLayout title={`${title} | Professional Capacity Building (PCB)`}>
  <Fragment slot="banner">
    Professional Capacity Building (PCB)
  </Fragment>
  <slot/>
</PageLayout>